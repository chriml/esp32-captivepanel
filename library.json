{
    "name": "ESP32_WifiManager",
    "version": "1.0.0",
    "description": "Generic captive portal ",
    "keywords": "wifi,captive,wifimanager,webserver",
    "homepage": "https://github.com/chriml/esp32-captivepanel",
    "repository": {
        "type": "git",
        "url": "https://github.com/chriml/esp32-captivepanel.git"
    },
    "authors": [
        {
            "name": "Christian Meinl",
            "maintainer": true
        }
    ],
    "license": "MIT",
    "frameworks": "arduino",
