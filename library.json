        "espressif32",
        "espressif8266",
        "raspberrypi"
    ],
    "dependencies": [
        {
            "name": "WiFi",
            "version": "*",
            "platforms": "espressif32"
        },
        {
            "name": "WebServer",
            "version": "*",
            "platforms": "espressif32"
        },
        {
            "name": "DNSServer",
            "version": "*",
            "platforms": "espressif32"
        },
        {
            "name": "WiFiClient",
            "version": "*",
            "platforms": "espressif32"
        },
        {
            "name": "ESPmDNS",
            "version": "*",
            "platforms": "espressif32"
        },
        {
            "name": "EEPROM",
            "version": "*",
            "platforms": "espressif32"
        },
        {
            "name": "https://github.com/me-no-dev/ESPAsyncWebServer.git",
            "version": "*",
            "platforms": "espressif32"
        }
    ],
    "export": {
        "include": [
            "examples",
            "src",
            "library.json",
            "library.properties",
            "LICENSE",
            "README.md"
        ]
    },
    "build": {
         "libLDFMode": "deep",
        "libCompatMode": "strict"
    }
}